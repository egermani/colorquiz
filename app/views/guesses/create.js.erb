paper.Project("#myCanvas");
var raster = new paper.Raster({
    source: "<%= j(@spot.image.img.url(:full)) %>"
});
var $canvas = $(paper.view.element);
$canvas.parent().attr('style', "width: " + "<%= j(@spot.image.width.to_s)%>");
